{
  "name": "todo-workbench",
  "version": "1.0.0",
  "description": "A TODO WorkBench application built with Electron",
  "main": "dist/main.js",
  "scripts": {
    "build": "npm run build:main && npm run build:renderer",
    "build:main": "tsc --project tsconfig.main.json",
    "build:renderer": "webpack --mode=development",
    "build:renderer:dev": "webpack serve --config webpack.dev.config.js",
    "dev:renderer": "npm run build:main && npm run build:renderer:dev",
    "dev:hot": "node dev-start.js",
    "start:dev": "electron . --dev",
    "start": "electron .",
    "start:build": "npm run build && electron .",
    "dev": "npm run build && electron . --dev",
    "server": "npm run build:main && node dist/backend/server.js",
    "clean": "rimraf dist",
    "type-check": "tsc --noEmit",
    "lint": "eslint src/**/*.ts src/**/*.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,css,less,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,css,less,json}\"",
    "lint:fix": "eslint src/**/*.ts src/**/*.tsx --fix",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nuclear-donkeyy/TODO-WorkBench.git"
  },
  "author": "hajimi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Nuclear-donkeyy/TODO-WorkBench/issues"
  },
  "homepage": "https://github.com/Nuclear-donkeyy/TODO-WorkBench#readme",
  "devDependencies": {
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.15",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@types/sqlite3": "^3.1.11",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "buffer": "^6.0.3",
    "concurrently": "^8.2.2",
    "copy-webpack-plugin": "^13.0.0",
    "css-loader": "^7.1.2",
    "electron": "^36.4.0",
    "eslint": "^8.54.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "events": "^3.3.0",
    "globalthis": "^1.0.4",
    "html-webpack-plugin": "^5.6.3",
    "less": "^4.2.0",
    "less-loader": "^12.2.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.5.3",
    "process": "^0.11.10",
    "react-refresh": "^0.14.2",
    "react-refresh-typescript": "^2.0.9",
    "rimraf": "^5.0.5",
    "stream-browserify": "^3.0.0",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.2",
    "typescript": "^5.3.0",
    "util": "^0.12.5",
    "wait-on": "^7.2.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.8.2",
    "antd": "^5.25.4",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dayjs": "^1.11.13",
    "express": "^4.18.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.2.0",
    "react-router-dom": "^6.30.1",
    "sqlite3": "^5.1.6"
  }
}
